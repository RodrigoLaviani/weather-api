{
  "name": "weather-api",
  "version": "1.0.0",
  "description": "Backend where you can check the weather of your city. ",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/app.js",
    "dev": "ts-node-dev --respawn index.ts -r --open",
    "test": "jest -u --collect-coverage --colors",
    "build": "tsc --sourceMap false",
    "insert-data": "ts-node-dev src/app/infrastructure/database/data/data-script.ts"
  },
  "jest": {
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RodrigoLaviani/weather-api.git"
  },
  "keywords": [
    "Weather",
    "Node",
    "Backend"
  ],
  "author": "Rodrigo Laviani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RodrigoLaviani/weather-api/issues"
  },
  "homepage": "https://github.com/RodrigoLaviani/weather-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.13",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.4.0",
    "@types/memory-cache": "^0.2.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "express-openapi": "^10.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.3.0",
    "typescript": "^4.5.4"
  }
}
